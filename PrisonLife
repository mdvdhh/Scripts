local WindUI
local ok, result = pcall(function()
    return require("./src/Init")
end)

if ok then
    WindUI = result
else
    WindUI = loadstring(game:HttpGet("https://raw.githubusercontent.com/Footagesus/WindUI/main/dist/main.lua"))()
end

local Window = WindUI:CreateWindow({
    Title = "Prison Life Hub",
    Author = "by SheScripts",
    Folder = "prisonlifehub",
    Icon = "sfsymbols:appleLogo",
    IconSize = 44,
    NewElements = true,
    HideSearchBar = false,
    OpenButton = {
        Title = "Open UI",
        CornerRadius = UDim.new(1,0),
        StrokeThickness = 3,
        Enabled = true,
        Draggable = true,
        OnlyMobile = false,
        Color = ColorSequence.new(Color3.fromHex("#FF69B4"), Color3.fromHex("#FF69B4"))
    }
})

local AimbotTab = Window:Tab({
    Title = "Aimbot",
    Icon = "target"
})

local Players = game:GetService("Players")
local LP = Players.LocalPlayer
local Cam = workspace.CurrentCamera
local RunService = game:GetService("RunService")

local aimbotOn = false
local FOVring = nil
local connection

local LockGuards = false
local LockInmates = false
local LockCriminals = false

AimbotTab:Toggle({
    Title = "Lock Onto Guards",
    Default = false,
    Callback = function(v)
        LockGuards = v
    end
})

AimbotTab:Toggle({
    Title = "Lock Onto Inmates",
    Default = false,
    Callback = function(v)
        LockInmates = v
    end
})

AimbotTab:Toggle({
    Title = "Lock Onto Criminals",
    Default = false,
    Callback = function(v)
        LockCriminals = v
    end
})

AimbotTab:Toggle({
    Title = "Turn on/off Aimbot",
    Default = false,
    Callback = function(state)
        aimbotOn = state
        local fov = 100

        if state then
            FOVring = Drawing.new("Circle")
            FOVring.Visible = true
            FOVring.Thickness = 2
            FOVring.Color = Color3.fromRGB(128,0,128)
            FOVring.Filled = false
            FOVring.Radius = fov
            FOVring.Position = Cam.ViewportSize / 2

            local function getTorso(char)
                return char:FindFirstChild("Head") or char:FindFirstChild("HumanoidRootPart")
            end

            local function isAlive(plr)
                local char = plr.Character
                if not char then return false end
                local hum = char:FindFirstChildOfClass("Humanoid")
                return hum and hum.Health > 0
            end

            local function allowedTeam(plr)
                if not plr.Team then return false end
                local teamName = plr.Team.Name
                if teamName == "Guards" and LockGuards then return true end
                if teamName == "Inmates" and LockInmates then return true end
                if teamName == "Criminals" and LockCriminals then return true end
                return false
            end

            local function wallCheck(part)
                local origin = Cam.CFrame.Position
                local dir = (part.Position - origin)
                local params = RaycastParams.new()
                params.FilterType = Enum.RaycastFilterType.Blacklist
                params.FilterDescendantsInstances = {LP.Character}
                local hit = workspace:Raycast(origin, dir, params)
                return not hit or hit.Instance:IsDescendantOf(part.Parent)
            end

            local function getClosest()
                local best = nil
                local bestDist = fov
                local center = Cam.ViewportSize / 2

                for _, plr in ipairs(Players:GetPlayers()) do
                    if plr ~= LP and isAlive(plr) and allowedTeam(plr) then
                        local char = plr.Character
                        if char then
                            local torso = getTorso(char)
                            if torso then
                                local pos, vis = Cam:WorldToViewportPoint(torso.Position)
                                local dist = (Vector2.new(pos.X,pos.Y) - center).Magnitude
                                if vis and dist < bestDist and wallCheck(torso) then
                                    bestDist = dist
                                    best = torso
                                end
                            end
                        end
                    end
                end

                return best
            end

            connection = RunService:BindToRenderStep("AimbotLoop", 301, function()
                if not aimbotOn then return end
                if FOVring then
                    FOVring.Position = Cam.ViewportSize / 2
                end
                local target = getClosest()
                if target then
                    Cam.CFrame = CFrame.new(Cam.CFrame.Position, target.Position)
                end
            end)

        else
            if connection then
                connection:Disconnect()
                connection = nil
            end
            if FOVring then
                FOVring:Remove()
                FOVring = nil
            end
        end
    end
})

local GunsTab = Window:Tab({
    Title = "Weapons/items",
    Icon = "sword"
})

local Players = game:GetService("Players")
local LP = Players.LocalPlayer
local Rep = game:GetService("ReplicatedStorage")

local function tpGun(cf, callback)
    local char = LP.Character or LP.CharacterAdded:Wait()
    local hrp = char:WaitForChild("HumanoidRootPart")
    local oldCF = hrp.CFrame
    hrp.CFrame = cf
    task.wait(0.35)
    if callback then
        pcall(callback)
    end
    task.wait(0.35)
    hrp.CFrame = oldCF
end

local function pickUpGun(gun)
    if gun then
        Rep.Remotes.InteractWithItem:InvokeServer(gun)
    end
end

